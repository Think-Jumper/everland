<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="urn:nhibernate-mapping-2.2">

  <class name="eland.model.User, eland.model" table="Users">

    <id	name="Id" access="property"	column="Id"	type="Guid">
      <generator class="guid" />
    </id>

    <property	name="OpenId"	access="property"	type="String" length="500">
      <column	name="OpenId" />
    </property>

    <property name="FirstName" access="property">
      <column name="FirstName" />
    </property>

    <property name="LastName" access="property">
      <column name="LastName" />
    </property>

  </class>

</hibernate-mapping>